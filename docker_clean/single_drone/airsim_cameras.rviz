      Value: /airsim_node/PX4_Drone1/front_depth/DepthPlanar
    Transport Hint: raw
  Global Options:
    Background Color: 48; 48; 48
    Fixed Frame: world_ned
  Tools:
  - Class: rviz_default_plugins/Interact
  - Class: rviz_default_plugins/MoveCamera
