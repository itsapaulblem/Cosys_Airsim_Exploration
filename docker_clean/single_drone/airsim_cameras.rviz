    Cell Size: 1
    Class: rviz_default_plugins/Grid
    Color: 160; 160; 164
    Enabled: true
    Name: Grid
  - Class: rviz_default_plugins/TF
    Enabled: true
    Name: TF
  - Class: rviz_default_plugins/Image
    Enabled: true
    Name: PX4_Drone1 front_rgb Scene
